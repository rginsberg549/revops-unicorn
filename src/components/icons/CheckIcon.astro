---
interface Props {
  class?: string;
}

const { class: className = "" } = Astro.props;
---

<svg class:list={["w-5 h-5 flex-shrink-0", className]} viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Gold circle background -->
  <circle cx="12" cy="12" r="10" fill="currentColor" opacity="0.15"/>
  <circle cx="12" cy="12" r="10" fill="none" stroke="currentColor" stroke-width="1.5"/>
  <!-- Checkmark -->
  <path d="M7 12.5l3.5 3.5 6.5-7" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
</svg>
