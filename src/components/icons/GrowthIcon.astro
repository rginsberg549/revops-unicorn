---
// Team Development / Growth icon
interface Props {
  class?: string;
}
const { class: className = "w-8 h-8" } = Astro.props;
---

<svg class={className} viewBox="0 0 32 32" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Person silhouette -->
  <circle cx="16" cy="10" r="5" fill="#1a365d"/>
  <path d="M8 28c0-4.4 3.6-8 8-8s8 3.6 8 8" fill="#1a365d"/>
  <!-- Growth arrow -->
  <path d="M20 6l6-2-2 6" fill="#c9a227"/>
  <path d="M14 14l6-8" stroke="#c9a227" stroke-width="2" stroke-linecap="round"/>
  <!-- Star -->
  <path d="M25 12l.7 1.4 1.5.2-1.1 1.1.3 1.5-1.4-.7-1.4.7.3-1.5-1.1-1.1 1.5-.2z" fill="#c9a227"/>
</svg>
