---
interface Props {
  class?: string;
}

const { class: className = "" } = Astro.props;
---

<svg class:list={["w-8 h-8 flex-shrink-0", className]} viewBox="0 0 32 32" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Browser window -->
  <rect x="3" y="5" width="26" height="22" rx="3" stroke="#1a365d" stroke-width="1.5" fill="none"/>
  <!-- Title bar -->
  <line x1="3" y1="11" x2="29" y2="11" stroke="#1a365d" stroke-width="1.5"/>
  <!-- Browser dots -->
  <circle cx="7" cy="8" r="1" fill="#c9a227"/>
  <circle cx="10.5" cy="8" r="1" fill="#1a365d" opacity="0.3"/>
  <circle cx="14" cy="8" r="1" fill="#1a365d" opacity="0.3"/>
  <!-- User icon in portal -->
  <circle cx="16" cy="17" r="3" stroke="#c9a227" stroke-width="1.5" fill="#c9a227" fill-opacity="0.15"/>
  <path d="M10 24c0-3.3 2.7-6 6-6s6 2.7 6 6" stroke="#c9a227" stroke-width="1.5" stroke-linecap="round" fill="none"/>
  <!-- Gold connection dot -->
  <circle cx="25" cy="8" r="1.5" fill="#c9a227"/>
</svg>
