---
// Clear Processes / Documentation icon
interface Props {
  class?: string;
}
const { class: className = "w-8 h-8" } = Astro.props;
---

<svg class={className} viewBox="0 0 32 32" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Document stack -->
  <rect x="8" y="6" width="16" height="20" rx="2" fill="#1a365d" opacity="0.15"/>
  <rect x="6" y="4" width="16" height="20" rx="2" fill="white" stroke="#1a365d" stroke-width="1.5"/>
  <!-- Lines -->
  <path d="M9 9h10M9 13h7M9 17h10" stroke="#1a365d" stroke-width="1.5" stroke-linecap="round"/>
  <!-- Gold checkmark badge -->
  <circle cx="22" cy="20" r="5" fill="#c9a227"/>
  <path d="M19.5 20l1.5 1.5 3-3" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
</svg>
